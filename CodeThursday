counterright=1;
counterleft=1;
scanf("%d", &length);

for(counterright = 1; counterright<length; counterright++){
scanf("%d", &x);
	if(counterright == 1){
		push(x, &stack);
		continue;
	}
	y = pop(&stack);
	if(x >= y){
	push(y, &stack);
	push(x, & stack);
	}
	if(x < y){
	push(y, &stack);
	
	//for(int i= 1; i<-length; i++){
		//z = pop(&stack);
		//w = pop(&stack);
		//if(z == w){
		counterleft++;
		//}
